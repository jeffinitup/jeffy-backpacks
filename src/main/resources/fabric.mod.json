{
  "schemaVersion": 1,
  "id": "jbp",
  "version": "${version}",
  "name": "Jeffy Backpacks",
  "description": "Jeffy Backpacks; they're better than bundles!",
  "authors": [
    "jeffyjamzhd"
  ],
  "contact": {
    "sources": "https://github.com/jeffinitup/jeffy-backpacks"
  },
  "license": "MIT",
  "icon": "assets/jbp/icon.png",
  "environment": "*",
  "entrypoints": {
    "btw:addon": [
      "com.jeffyjamzhd.jeffybackpacks.JeffyBackpacks"
    ]
  },
  "custom": {
    "btw:addon_prefix": "JBP",
    "loom:injected_interfaces": {
      "net/minecraft/src/GuiScreen": ["com/jeffyjamzhd/jeffybackpacks/api/impl/IGuiScreen"],
      "net/minecraft/src/ItemArmor": ["com/jeffyjamzhd/jeffybackpacks/api/impl/IItemArmor"]
    }
  },
  "mixins": [
    "jbp.mixins.json"
  ],
  "accessWidener": "jbp.accessWidener",
  "depends": {
    "fabricloader": ">=0.15.6",
    "minecraft": "1.6.4"
  }
}
