{
  "schemaVersion": 1,
  "id": "jbp",
  "version": "${version}",
  "name": "Jeffy Backpacks",
  "description": "Jeffy Backpacks; they're better than bundles!",
  "authors": [
    "jeffyjamzhd"
  ],
  "contact": {
    "homepage": "https://github.com/jeffinitup/jeffy-backpacks"
  },
  "license": "MIT",
  "icon": "assets/jbp/icon.png",
  "environment": "*",
  "entrypoints": {
    "btw:addon": [
      "com.jeffyjamzhd.jeffybackpacks.JeffyBackpacks"
    ],
    "emi": [
      "com.jeffyjamzhd.jeffybackpacks.emi.JeffyBackpacksEMI"
    ]
  },
  "custom": {
    "btw:addon_prefix": "JBP",
    "loom:injected_interfaces": {
      "net/minecraft/src/InventoryPlayer": ["com/jeffyjamzhd/jeffybackpacks/api/impl/IInventoryPlayer"],
      "net/minecraft/src/EntityPlayer": ["com/jeffyjamzhd/jeffybackpacks/api/impl/IEntityPlayer"],
      "btw/util/status/StatusEffectBuilder": ["com/jeffyjamzhd/jeffybackpacks/api/impl/IStatusEffectBuilder"],
      "btw/util/status/StatusEffect": ["com/jeffyjamzhd/jeffybackpacks/api/impl/IStatusEffect"]
    }
  },
  "mixins": [
    "jbp.mixins.json"
  ],
  "accessWidener": "jbp.accessWidener",
  "depends": {
    "fabricloader": ">=0.15.6",
    "minecraft": "1.6.4",
    "jl": ">=0.1.0"
  }
}
